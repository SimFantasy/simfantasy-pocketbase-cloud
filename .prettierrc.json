{
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "insertPragma": false,
  "tabWidth": 2,
  "useTabs": false,
  "endOfLine": "auto",
  "HTMLWhitespaceSensitivity": "ignore",
  "extends": ["eslint:recommended", "plugin:prettier/recommended"]
}
